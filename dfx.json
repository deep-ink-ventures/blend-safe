{
  "canisters": {
    "blend_safe_backend": {
      "candid": "src/blend_safe_backend/blend_safe_backend.did",
      "package": "blend_safe_backend",
      "type": "rust"
    },
    "blend_safe_frontend": {
      "dependencies": [
        "blend_safe_backend"
      ],
      "frontend": {
        "entrypoint": "src/blend_safe_frontend/src/index.html"
      },
      "source": [
        "src/blend_safe_frontend/assets",
        "dist/blend_safe_frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}